{% extends "@SocialNetwork/medias/instagram/medias.html.twig" %}

{% block cardImage %}
    {% if media.image is not null %}
        <img class="card-img-top" src="{{ asset(media.image.assetPath) }}"/>
    {% else %}
        <img class="card-img-top" src="{{ media.previewUrl }}"/>    
    {% endif %}
    
    
{% endblock %}